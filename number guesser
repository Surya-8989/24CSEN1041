import random

number = random.randint(1, 10)
attempts = 3

while attempts > 0:
    guess = int(input("Guess the number (1-10): "))
    if guess == number:
        print("ğŸ‰ Correct! You win!")
        break
    elif guess < number:
        print("â¬† Too low!")
    else:
        print("â¬‡ Too high!")
    attempts -= 1

if attempts == 0:
    print(f"ğŸ’€ Game over! The number was {number}")
